<h1><%= @station.name %></h1>
<h3>Associated Lines:</h3>
  <ul>
    <% @station.lines.each do |line| %>
      <li><%= line.name %></li>
    <% end %>
  </ul>
<p>
  <%= link_to "Edit station", edit_station_path(@station), class: 'btn btn-info' %>
</p>

<p>
  <%= link_to "Delete station", station_path(@station),
                                class: 'btn btn-danger',
                                data: {confirm: 'Woah, really?', method: 'delete'} %>
</p>
